#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/root/Desktop/blog_root/halfmantou_root/node_modules/.store/base64url@1.0.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/root/Desktop/blog_root/halfmantou_root/node_modules/.store/base64url@1.0.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../base64url@1.0.6/node_modules/base64url/bin/base64url" "$@"
else
  exec node  "$basedir/../../../../../base64url@1.0.6/node_modules/base64url/bin/base64url" "$@"
fi
