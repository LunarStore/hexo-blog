#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/root/Desktop/blog_root/hexo-blog/node_modules/.store/cargo-cp-artifact@0.1.9/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/root/Desktop/blog_root/hexo-blog/node_modules/.store/cargo-cp-artifact@0.1.9/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cargo-cp-artifact@0.1.9/node_modules/cargo-cp-artifact/bin/cargo-cp-artifact.js" "$@"
else
  exec node  "$basedir/../../../../../cargo-cp-artifact@0.1.9/node_modules/cargo-cp-artifact/bin/cargo-cp-artifact.js" "$@"
fi
