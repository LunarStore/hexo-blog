#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/root/Desktop/blog_root/halfmantou_root/node_modules/.store/repeating@1.1.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/root/Desktop/blog_root/halfmantou_root/node_modules/.store/repeating@1.1.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../repeating@1.1.3/node_modules/repeating/cli.js" "$@"
else
  exec node  "$basedir/../../../../../repeating@1.1.3/node_modules/repeating/cli.js" "$@"
fi
